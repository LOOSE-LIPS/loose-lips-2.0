{
  "name": "@sveltejs/kit",
  "version": "1.0.0-next.165",
  "type": "module",
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.22",
    "cheap-watch": "^1.0.3",
    "sade": "^1.7.4",
    "vite": "^2.5.6"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.4.2",
    "@types/amphtml-validator": "^1.0.1",
    "@types/cookie": "^0.4.0",
    "@types/marked": "^2.0.2",
    "@types/micromatch": "^4.0.2",
    "@types/mime": "^2.0.3",
    "@types/node": "^14.14.43",
    "@types/rimraf": "^3.0.0",
    "@types/sade": "^1.7.2",
    "amphtml-validator": "^1.0.34",
    "cookie": "^0.4.1",
    "devalue": "^2.0.1",
    "eslint": "^7.25.0",
    "kleur": "^4.1.4",
    "locate-character": "^2.0.5",
    "marked": "^2.0.3",
    "micromatch": "^4.0.4",
    "mime": "^2.5.2",
    "node-fetch": "^3.0.0-beta.9",
    "port-authority": "^1.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.0",
    "selfsigned": "^1.10.11",
    "sirv": "^1.0.12",
    "svelte": "^3.42.4",
    "svelte-check": "^2.2.4",
    "svelte2tsx": "~0.4.1",
    "tiny-glob": "^0.2.8",
    "uvu": "^0.5.1"
  },
  "peerDependencies": {
    "svelte": "^3.39.0"
  },
  "bin": {
    "svelte-kit": "svelte-kit.js"
  },
  "files": [
    "assets",
    "dist",
    "types",
    "svelte-kit.js"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./ssr": {
      "import": "./dist/ssr.js"
    },
    "./node": {
      "import": "./dist/node.js"
    },
    "./hooks": {
      "import": "./dist/hooks.js"
    },
    "./install-fetch": {
      "import": "./dist/install-fetch.js"
    },
    "./types": "./types/index.d.ts"
  },
  "types": "types/index.d.ts",
  "engines": {
    "node": "^12.20 || >=14.13"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -cw",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern \"src/packaging/test/**\" \"src/**/*.{ts,mjs,js,svelte}\" && npm run check-format",
    "check": "tsc && svelte-check --ignore \"src/packaging/test\"",
    "format": "npm run check-format -- --write",
    "check-format": "prettier --check . --config ../../.prettierrc --ignore-path .gitignore",
    "test": "npm run test:unit && npm run test:packaging && npm run test:integration",
    "test:unit": "uvu src \"(spec\\.js|test[\\\\/]index\\.js)\" -i packaging",
    "test:packaging": "uvu src/packaging \"(spec\\.js|test[\\\\/]index\\.js)\"",
    "test:integration": "uvu test test.js"
  }
}